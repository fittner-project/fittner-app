{
  "expo": {
    "name": "피트너",
    "slug": "fittner-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fittner-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "kr.co.fittner",
      "buildNumber": "1.0.1",
      "name": "피트너",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "permissions": ["POST_NOTIFICATIONS"],
      "googleServicesFile": "./google-services.json",
      "package": "kr.co.fittner"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["@react-native-firebase/app"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f30127d6-bd30-4718-bec6-7efe790f9265"
      }
    }
  }
}
